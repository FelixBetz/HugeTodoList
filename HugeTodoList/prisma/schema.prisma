generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model TodoItem {
  id     Int     @id @default(autoincrement())
  createdId Int @unique
  title  String  
  description String
  isDone   Boolean
  categoryId Int
  modifiedDate  Int

}

